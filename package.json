{
  "name": "@getvibeguard/cli",
  "version": "1.1.0",
  "type": "module",
  "bin": {
    "vibeguard": "./dist/cli.js"
  },
  "scripts": {
    "build:cli": "tsc",
    "build:dashboard": "npm run build --prefix src/dashboard",
    "copy:assets": "node scripts/copy-dashboard.js",
    "build": "npm run build:cli && npm run build:dashboard && npm run copy:assets",
    "prepublishOnly": "npm run build",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "chokidar": "^5.0.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "inquirer": "^9.2.15",
    "open": "^10.1.0",
    "simple-git": "^3.30.0",
    "sirv": "^2.0.4",
    "write-file-atomic": "^5.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^25.0.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
